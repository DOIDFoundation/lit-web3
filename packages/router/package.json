{
  "name": "@lit-web3/router",
  "version": "0.2.1",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "description": "A wrapper for @lit-labs/router with hash mode",
  "scripts": {
    "build": "tsup src/*.ts --outDir ./dist --format esm --dts --clean",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@lit-web3/base": "workspace:latest"
  },
  "devDependencies": {
    "@lit-labs/router": "^0.1.3",
    "tsup": "^8.0.1",
    "urlpattern-polyfill": "^9.0.0"
  },
  "peerDependencies": {
    "lit": "^3"
  }
}
