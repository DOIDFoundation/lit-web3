{
  "name": "@doid/core",
  "version": "0.12.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./http": "./src/http/index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./*": "./dist/*.js",
      "./http": "./dist/http/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/**/*.js src/**/*.ts --outDir ./dist --format esm --dts --clean --minify",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@lit-app/state": "^0.8.4",
    "lodash.memoize": "^4.1.2",
    "safe-decode-uri-component": "^1.2.1"
  },
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.1"
  }
}
