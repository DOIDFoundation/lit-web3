{
  "name": "@lit-web3/extension",
  "displayName": "DOID Extension",
  "version": "0.0.2",
  "description": "DOID Extension",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:inpage": "npm run build:inpage -- --watch --mode development",
    "dev:app": "vite --host --mode development",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:inpage": "vite build --config vite.config.inpage.ts",
    "build:app": "vite build",
    "bundle": "cross-env NODE_ENV=production run-p clear && run-p build && run-p bundle:*",
    "bundle:zip": "rimraf DOID.$npm_package_version.zip && jszip-cli add ./dist/* -o ./DOID.$npm_package_version.zip",
    "bundle:crx": "crx pack ./dist -o ./DOID.$npm_package_version.crx",
    "clear": "rimraf ./DOID.$npm_package_version.zip && rimraf ./DOID.$npm_package_version.crx",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [],
  "dependencies": {
    "@aptos-labs/wallet-adapter-core": "2.0.1",
    "@chainify/evm": "^2.2.0",
    "@keystonehq/metamask-airgapped-keyring": "^0.9.1",
    "@libp2p/crypto": "^1.0.7",
    "@lit-web3/core": "workspace:latest",
    "@lit-web3/dui": "workspace:latest",
    "@lit-web3/ethers": "workspace:latest",
    "@lit-web3/solana-wallet-standard": "workspace:latest",
    "@metamask/approval-controller": "^2.0.0",
    "@metamask/base-controller": "^2.0.0",
    "@metamask/browser-passworder": "^4.0.2",
    "@metamask/eth-json-rpc-middleware": "^11.0.0",
    "@metamask/eth-json-rpc-provider": "^1.0.0",
    "@metamask/eth-keyring-controller": "^10.0.1",
    "@metamask/eth-sig-util": "^5.0.2",
    "@metamask/etherscan-link": "^2.2.0",
    "@metamask/object-multiplex": "^1.2.0",
    "@metamask/obs-store": "^7.0.0",
    "@metamask/permission-controller": "^3.0.0",
    "@metamask/post-message-stream": "^6.1.0",
    "@metamask/providers": "^10.2.1",
    "@metamask/safe-event-emitter": "^2.0.0",
    "@metamask/subject-metadata-controller": "^2.0.0",
    "@scure/bip39": "^1.2.0",
    "@solana/web3.js": "^1.73.3",
    "aptos": "^1.7.1",
    "await-semaphore": "^0.1.3",
    "debounce-stream": "^2.0.0",
    "ed25519-hd-key": "^1.3.0",
    "end-of-stream": "^1.4.4",
    "eth-block-tracker": "^7.0.0",
    "eth-json-rpc-filters": "^6.0.0",
    "eth-json-rpc-middleware": "^9.0.1",
    "eth-rpc-errors": "^4.0.3",
    "eventemitter3": "^5.0.0",
    "fast-deep-equal": "^3.1.3",
    "ipfs-core": "^0.18.0",
    "ipfs-http-client": "^60.0.0",
    "is-stream": "^3.0.0",
    "json-rpc-engine": "^6.1.0",
    "json-rpc-middleware-stream": "^4.2.1",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.1",
    "pify": "^6.1.0",
    "process-nextick-args": "^2.0.1",
    "promise-to-callback": "^1.0.0",
    "pump": "^3.0.0",
    "reselect": "^4.1.7",
    "swappable-obj-proxy": "^1.1.0",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.0",
    "w3name": "^1.0.8",
    "web3.storage": "^4.4.0",
    "webext-bridge": "^6.0.1"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.18",
    "@ffflorian/jszip-cli": "^3.4.0",
    "@types/chrome": "latest",
    "@types/end-of-stream": "^1.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/pump": "^1.1.1",
    "@types/uuid": "^9.0.1",
    "@types/webextension-polyfill": "^0.10.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "kolorist": "^1.6.0",
    "rimraf": "^5.0.0",
    "unplugin-auto-import": "^0.15.0",
    "vite-plugin-node-polyfills": "^0.8.2",
    "webextension-polyfill": "^0.10.0"
  },
  "license": "MIT"
}
