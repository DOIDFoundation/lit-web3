  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.13",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/end-of-stream": "^1.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/pump": "^1.1.1",
    "@types/webextension-polyfill": "^0.10.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "kolorist": "^1.6.0",
    "unplugin-auto-import": "^0.15.0"
  },
  "license": "MIT"
